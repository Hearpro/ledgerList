    "@testing-library/react": "^12.1.5",
    "@types/node": "^18.11.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.26",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "autoprefixer": "^9.8.8",
    "compressing": "^1.6.2",
    "form-data": "^4.0.0",
    "lint-staged": "^10.0.7",
    "postcss": "^7.0.39",
    "prettier": "^2.2.0",
    "pump": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "styled-components": "^5"
