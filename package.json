{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@babel/preset-env": "^7.19.3",
    "antd-mobile": "^5.24.0",
    "i18next": "^21.6.14",
    "react": "17.x",
    "react-dom": "17.x",
    "react-i18next": "^11.16.2",
    "styled-components": "^5.3.6",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.26",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "autoprefixer": "^9.8.8",
    "lint-staged": "^10.0.7",
    "postcss": "^7.0.39",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
